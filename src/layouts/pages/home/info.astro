---
import Home from "../../paths/home.astro";

import { Required } from "../../../defaults";
const { lang, filename, meta_title, meta_desc, meta_img, nav_home, nav_info, nav_gall, nav_news, nav_comm, nav_down, nav_join, nav_dl, credit, links } = Astro.props as Required

export interface PageRequired {
        card_no: string
        cnd: string
        holder: string
        info_cst: string
        info_cst_desc: string
        info_desc: string
        info_pic: string
        info_pic_desc: string
        info_yrs: string
        info_yrs_desc: string
        mbbank: string
        story: string
        story_label: string
        timeline: string
        tl_1: string
        tl_1_desc: string
        tl_2: string
        tl_2_desc: string
        tl_3: string
        tl_3_desc: string
        tl_4: string
        tl_4_desc: string
        tl_5: string
        tl_5_desc: string
        tl_6: string
        tl_6_desc: string
}
const { card_no, cnd, holder, info_cst, info_cst_desc, info_desc, info_pic, info_pic_desc, info_yrs, info_yrs_desc, mbbank, story, story_label, timeline, tl_1, tl_1_desc, tl_2, tl_2_desc, tl_3, tl_3_desc, tl_4, tl_4_desc, tl_5, tl_5_desc, tl_6, tl_6_desc } = Astro.props as PageRequired

---
<Home lang={lang} filename={filename} meta_title={meta_title} meta_desc={meta_desc} meta_img={meta_img} nav_home={nav_home} nav_info={nav_info} nav_gall={nav_gall} nav_news={nav_news} nav_comm={nav_comm} nav_down={nav_down} nav_join={nav_join} nav_dl={nav_dl} credit={credit} links={links}>
<script type="text/javascript"></script>
<div class="gr">
  <img src="/res/img/logongang-white-min.png" />
  <h1>{info_desc}</h1>
  <span
    class="show"
    style="
      top: 0;
      left: 0;
      width: 22vw;
      height: 12vh;
      background: #002049;
      border-radius: 0 0 100vmax 0;
      z-index: 2;
      animation: sptop 0.5s ease-out 0.2s;
      animation-fill-mode: forwards !important;
      transform: translateY(-16vh);
