---
import "./css/main.css"

import { Required } from "../defaults";
const { lang = "vi", filename, meta_title = "Vietnam", meta_desc, meta_img = "/placeholder", nav_home, nav_info, nav_gall, nav_news, nav_comm, nav_down, nav_join, nav_dl, credit, links } = Astro.props as Required
const title="Team Fuho " + (filename != "index" ? "- " : "") + meta_title,
    exlang = lang=="vi"?"":lang
---

