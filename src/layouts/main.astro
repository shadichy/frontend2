var tbh = true
  const scroll =
      window.requestAnimationFrame ||
      function (callback) {
        window.setTimeout(callback, 1000 / 60)
      },
      nav = document.querySelector(".nav"),
      root = document.documentElement
  
  let reqheight = 0,
      reqmin = 0,
      reqmax = 0
const 
  customfunc = [],
  isElementInViewport = (e) => {
    let rect = e.getBoundingClientRect(),
      clientHeight = window.innerHeight || document.documentElement.clientHeight
    return (
      (rect.top <= 0 && rect.bottom >= 0) ||
      (rect.bottom >=
        clientHeight &&
        rect.top <=
          clientHeight) ||
      (rect.top >= 0 &&
        rect.bottom <=
          clientHeight)
    )
  }

function fscr() {
  let b = window.scrollY
  nav.style.top =
    b == reqheight || (b >= reqmin && b <= reqmax) ? "-10vh" : "0"
  scroll(fscr)
}

function tscr() {
  let b = window.scrollY
  let spn = document.querySelector("footer>div:first-child>span");
  spn.style.opacity =
    b == reqheight || (b >= reqmin && b <= reqmax) ? "0" : "1"
  scroll(tscr)
}
    
window.addEventListener("DOMContentLoaded", e => {
  let currentpage=document.querySelector(`a.txtd.d${filename}`)
  currentpage.style.color = "color: #ff7518!important;"
  document.getElementById("butt").onclick = ()=>{
    let xtab = document.getElementById("tab"),
      swipe = document.getElementById("swipe"),
      rat = window.innerWidth / window.innerHeight,
      buttonSvg = document.querySelector("#butt>svg"),
      button = document.getElementById("butt"),
      d=[]
    if (rat <= 133 / 100) {
      (tbh) ? d=[100,180,"rgba(255, 255, 255, 0.8)"] : d=[0,0,"none"]
      swipe.style.width = `${d[0]}vw`
      buttonSvg.style.transform = `rotate(${d[1]}deg)`
      button.style.background = d[2]
      tbh = !tbh
    } else {
      swipe.style.width = "40vw"
    }
  }

  function loop() {
    document.querySelectorAll(".show-on-scroll").forEach(function (element) {
      if (isElementInViewport(element)) {
        element.classList.add("is-visible")
      } else {
        element.classList.remove("is-visible")
      }
    })
    // scroll(loop)
  }

  document.onscroll = loop
