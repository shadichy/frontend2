  // 	fetch(where)
  // 		.then(r=>r.text())
  // 		.then(res => {
  // 			document.getElementById(target).innerHTML = res
  // 			if (callback) callback()
  // 		})
  // }

  // function strdom(s) {
  // 	return new DOMParser().parseFromString(s, 'text/html').body.firstChild
  // }
  // function chklod(target, where, callback) {
  // 	fetch(where)
